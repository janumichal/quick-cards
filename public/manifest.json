{
    "manifest_version": 2,
    "name": "Quick shortcuts",
    "version": "1.0",
    "description": "Changes the new tab page to customizable one.",

    "chrome_settings_overrides" : {
      "homepage": "index.html"
     },
     "chrome_url_overrides" : {
      "newtab": "index.html"
    },
    "permissions":[
      "bookmarks",
      "tabs",
      "storage",
      "menus"
    ]
  }
  